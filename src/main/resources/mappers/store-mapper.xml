<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="StoreMapper">
	<insert id="insertStore">
	INSERT INTO STORE_TBL VALUES(SEQ_STORE_NO.NEXTVAL,#{storeName},#{storeLocation},#{storeCeo},#{storeTel},#{storeUrl},#{storeAccount},#{storeFilename},DEFAULT)
	</insert>
	<delete id="deleteStore">
	DELETE FROM STORE_TBL WHERE STORE_ID = #{storeNo}
	</delete>
</mapper>